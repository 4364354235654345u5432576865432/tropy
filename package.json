    "js-yaml": "^3.4.6",
    "moment": "^2.12.0",
    "ms": "^0.7.1",
    "object.entries": "^1.0.3",
    "object.values": "^1.0.3",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "sqlite3": "^3.1.1",
    "string.prototype.padstart": "^3.0.0",
    "transducers.js": "^0.3.2",
    "winston": "^2.1.1",
    "write-file-atomic": "^1.1.4",
    "yargs": "^4.3.2"
  }

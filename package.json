{
  "name": "tropy",
  "productName": "Tropy",
  "version": "0.0.1",
  "private": true,
  "description": "Tropy is your friendly neighbourhood research assistant.",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/tropy/tropy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tropy/tropy.git"
  },
  "bugs": {
    "url": "https://github.com/tropy/tropy/issues"
  },
  "keywords": [],
  "author": {
    "name": "Center for History and New Media, George Mason University",
    "url": "https://chnm.gmu.edu"
  },
  "contributors": [
    "Sylvester Keil <sylvester@keil.or.at>"
  ],
  "engines": {
    "node": ">=5"
  },
  "main": "lib/browser/main.js",
  "scripts": {
    "branding": "node --harmony --harmony_destructuring --harmony_default_parameters scripts/electron branding --",
    "compile": "npm run make compile",
    "dist": "node --harmony --harmony_destructuring --harmony_default_parameters scripts/dist",
    "pack": "node --harmony --harmony_destructuring --harmony_default_parameters scripts/dist pack --",
    "lint": "npm run make lint || true",
    "make": "node --harmony --harmony_destructuring --harmony_default_parameters scripts/make",
    "db": "node --harmony --harmony_default_parameters --harmony_destructuring scripts/db",
    "prestart": "npm run make compile link",
    "start": "electron -e development",
    "poststart": "npm run make unlink",
    "rebuild": "node --harmony_destructuring --harmony_default_parameters scripts/rebuild",
    "postinstall": "npm run rebuild",
    "test": "npm run make test",
    "prewatch": "npm run make compile",
    "watch": "node scripts/watch"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-eslint": "^6.0.0",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-plugin-transform-async-to-module-method": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "chokidar": "^1.5.0",
    "devtron": "^1.0.0",
    "electron-mocha": "^2.0.0",
    "electron-packager": "^7.0.0",
    "eslint": "^2.4.0",
    "glob": "^7.0.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "shelljs": "^0.6.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "sqleton": "^1.0.0"
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "colors": "^1.1.2",
    "electron-prebuilt": "^1.0.1",

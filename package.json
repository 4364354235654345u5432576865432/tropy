{
  "name": "tropy",
  "productName": "Tropy",
  "version": "2.0.0-dev",
  "private": true,
  "description": "Tropy is your friendly neighbourhood research assistant.",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/tropy/tropy#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tropy/tropy"
  },
  "bugs": {
    "url": "https://github.com/tropy/tropy/issues"
  },
  "keywords": [],
  "author": {
    "name": "Center for History and New Media, George Mason University",
    "url": "https://chnm.gmu.edu"
  },
  "contributors": [
    "Sylvester Keil <sylvester@keil.or.at>",
    "Johannes Krtek <johannes.krtek@flachware.com>"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "main": "lib/browser/main.js",
  "scripts": {
    "clean": "rimraf coverage doc lib .nyc_output npm-debug.log",
    "compile": "node scripts/compile",
    "db": "node scripts/db",
    "prebuild": "npm run clean && npm run compile",
    "build": "node scripts/build",
    "postbuild": "node scripts/sign",
    "prepack": "npm run build",
    "pack": "node scripts/pack",
    "purge": "npm run clean && rimraf dist",
    "devtools": "electron scripts/devtools --user-data tmp",
    "lint": "npm run -s lint:js && npm run -s lint:css",
    "lint:js": "eslint --color src test scripts",
    "lint:css": "stylelint src/stylesheets/**/*.scss",
    "log:view": "cat tmp/log/tropy.log | node scripts/log-viewer",
    "log:tail": "tail -f tmp/log/tropy.log | node scripts/log-viewer",
    "prestart": "npm run -s compile",
    "start": "electron --app . --env development",
    "rebuild": "node scripts/rebuild",
    "postinstall": "npm run -s rebuild",
    "precoverage": "rimraf coverage .nyc_output && mkdir .nyc_output",
    "coverage": "env COVERAGE=true npm run -s test",
    "postcoverage": "nyc report -r text-summary -r html -r lcov",
    "test": "npm run -s test:main && npm run -s test:renderer",
    "test:main": "electron-mocha \"test/{browser,common}/**/*_test.js\"",
    "test:renderer": "electron-mocha --renderer \"test/{,!(browser)/**}/*_test.js\"",
    "watch": "node scripts/watch"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.0",
    "devtron": "^1.0.0",
    "electron": "^4.1.5",
    "electron-mocha": "^8.0.1",
    "electron-packager": "^13.1.1",
    "electron-winstaller": "^3.0.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "legal-eagle": "^0.16.0",
    "ms": "^2.1.1",
    "node-fetch": "^2.5.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.0.0",
    "prosemirror-test-builder": "^1.0.1",
    "pump": "^3.0.0",
    "react-dnd-test-backend": "^7.4.4",
    "react-test-renderer": "^16.8.6",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "split2": "^3.1.1",
    "sqleton": "^1.4.0",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.6.1"
  },
  "dependencies": {
    "@inukshuk/exif": "^2.0.0",
    "@pixi/filter-adjustment": "^2.7.0",
    "@tweenjs/tween.js": "^17.3.0",
    "bluebird": "^3.5.4",
    "classnames": "^2.2.6",
    "decompress": "^4.2.0",
    "desktop-idle": "^1.1.2",
    "edtf": "^2.6.1",
    "generic-pool": "^3.7.1",
    "js-yaml": "^3.13.1",
    "jsonld": "^1.6.0",
    "lodash.debounce": "^4.0.7",
    "lodash.throttle": "^4.1.1",
    "memoize-one": "^5.0.4",
    "mkdirp": "^0.5.1",
    "n3": "^1.1.1",
    "pino": "^5.12.3",
    "pixi.js": "^4.8.7",
    "prop-types": "^15.7.2",
    "prosemirror-commands": "^1.0.7",
    "prosemirror-gapcursor": "^1.0.3",
    "prosemirror-history": "^1.0.4",
    "prosemirror-inputrules": "^1.0.1",
    "prosemirror-model": "^1.7.0",
    "prosemirror-schema-basic": "^1.0.1",
    "prosemirror-schema-list": "^1.0.3",
    "prosemirror-state": "^1.2.2",
    "prosemirror-view": "^1.8.9",
    "react": "^16.8.6",
    "react-dnd": "^7.4.5",
    "react-dnd-electron-backend": "^1.7.0",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-redux": "^7.0.3",
    "react-transition-group": "^4.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3",
    "sanitize-filename": "^1.6.1",
    "sharp": "^0.22.1",
    "shortid": "^2.2.14",
    "sqlite3": "^4.0.6",
    "tiff": "^3.0.1",
    "transducers.js": "^0.3.2",
    "uuid": "^3.3.2",
    "winreg": "^1.2.4",
    "write-file-atomic": "^2.4.2",
    "yargs": "^13.2.2"
  }
}

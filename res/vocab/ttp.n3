@prefix ttp: <https://tropy.org/schema/v1/ttp#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<https://tropy.org/schema/v1/ttp#> a owl:Ontology ;
  vann:preferredNamespacePrefix "ttp" ;
  vann:preferredNamespaceUri "https://tropy.org/schema/v1/ttp#" ;
  dc:title "Tropy Template Vocabulary"@en ;
  dc:description "The RDF vocabulary for https://tropy.org/schema/v1/ttp defines the core elements used by Tropy templates."@en ;
  dc:issued "2017-06-23"^^xsd:date ;
  dc:publisher "Roy Rosenzweig Center for History and New Media, George Mason University" .

ttp:Template a rdfs:Class ;
  rdfs:label "Tropy Template"@en ;
  rdfs:isDefinedBy <https://tropy.org/schema/v1/ttp#> ;
  rdfs:comment "A Tropy template."@en .

ttp:Field a rdfs:Class ;
  rdfs:label "Tropy Template Field"@en ;
  rdfs:isDefinedBy <https://tropy.org/schema/v1/ttp#> ;
  rdfs:domain ttp:Template ;
  rdfs:comment "A Tropy template field."@en .


ttp:name a rdf:Property ;
  rdfs:label "Name"@en ;
  rdfs:isDefinedBy <https://tropy.org/schema/v1/ttp#> ;
  rdfs:subPropertyOf dc:title ;
  rdfs:domain ttp:Template ;
  rdfs:range rdfs:Literal ;
  rdfs:comment "A template name."@en .

ttp:type a rdf:Property ;
  rdfs:label "Type"@en ;
  rdfs:isDefinedBy <https://tropy.org/schema/v1/ttp#> ;
  rdfs:subPropertyOf dc:type ;
  rdfs:domain ttp:Template ;
  rdfs:comment "The type of object, in Tropy, which the template describes."@en .

ttp:domain a rdf:Property ;
  rdfs:label "Domain"@en ;
  rdfs:isDefinedBy <https://tropy.org/schema/v1/ttp#> ;
  rdfs:subPropertyOf rdfs:domain ;
  rdfs:domain ttp:Template ;
  rdfs:comment "Denotes a valid class for the item described by the template."@en .

ttp:field a rdf:Property ;
  rdfs:label "Field"@en ;
  rdfs:isDefinedBy <https://tropy.org/schema/v1/ttp#> ;
  rdfs:domain ttp:Template ;
  rdfs:range ttp:Field ;
  rdfs:comment "Describes a field in the template."@en .

ttp:property a rdf:Property ;
  rdfs:label "Property"@en ;
  rdfs:isDefinedBy <https://tropy.org/schema/v1/ttp#> ;
  rdfs:domain ttp:Field ;
  rdfs:range rdf:Property ;
  rdfs:comment "Denotes the field's property."@en .
